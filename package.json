{
  "name": "react-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/dialog": "^0.6.1",
    "@reach/tabs": "^0.6.1",
    "@reach/visually-hidden": "^0.6.1",
    "autoprefixer": "^9.7.2",
    "formik": "^2.0.4",
    "gotrue-js": "^0.9.25",
    "netlify-identity-widget": "^1.5.5",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.23",
    "postcss-cli": "^6.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-netlify-identity": "^0.1.9",
    "react-netlify-identity-widget": "^0.2.5",
    "react-popper": "^1.3.6",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "tailwindcss": "^1.1.3",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:css start:**",
    "build": "npm-run-all build:**",
    "build:css": "postcss src/styles/tailwind.css -o src/index.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/index.css -w",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "start:lambda": "NODE_ENV=development netlify-lambda serve src/lambda",
    "build:lambda": "netlify-lambda build src/lambda",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-fetch": "^2.6.0"
  }
}
